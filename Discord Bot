import Brainshop
import discord
brain = Brainshop.Brain(key="wOzuyiNgqmMPAwio" , bid =169570)
intents = discord.Intents.default()
intents.message_content = True
invite ="https://discord.com/api/oauth2/authorize?client_id=1066027178933891142&permissions=274877999136&scope=bot%20applications.commands"
msgtosend = "***Zorr Info***\n\nCreators : **Siddharth. S, Mithun .T & Asvin .H**\nProject on : Discord Chat-Bot Developed in Python\nStarted on : 18/1/2023\nEnded on : 22/1/2023\nServer : https://discord.gg/29qCcKnzg9\nUsername : Project Zorr#7032\nBot Name : Zorr\nBot Type : Smart Bot\nBot Command Prefix : z.\nBot Link : "+invite
client = discord.Client(intents=intents)
@client.event
async def on_ready():
    print("ready")
@client.event
async def on_message(message):
    if message.author == client.user:
        return
    if message.content.startswith("z."):
        content = message.content.lower().split("z.")[1]
        if content.startswith("help"):
            await message.channel.send("**Zorr Help Menu**\n\nCommands:-\n»z.help\n» z.info\n» z.invite")
        if content.startswith("info"):
            await message.channel.send(msgtosend)
        if content.startswith("invite"):
            await message.channel.send(invite)
    elif message.channel.id == 1065164649978413084:
        lkg = brain.ask(message.content)
        print(lkg,"\n\n")
        await message.channel.send(lkg)
        print("sent")
client.run("MTA2NjAyNzE3ODkzMzg5MTE0Mg.G-jTNM.XcoHqQrOyNbkXMd3AK_Tc27HtppKcVa_g5okr4")
